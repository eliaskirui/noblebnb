<h1 class="font-bold text-2xl mb-4">Listings</h1>
<div class="mb-4">
  <%== pagy_nav(@pagy) %>
</div>
<ul role="list" className="grid grid-cols-2 gap-x-4 gap-y-8 sm:grid-cols-3 sm:gap-x-6 lg:grid-cols-4 xl:gap-x-8">
  <% @listings.each do |listing| %>
    <li key={file.source} className="relative">
    <a href="<%= listing_path(listing) %>" className="group block w-full aspect-w-10 aspect-h-7 rounded-lg bg-gray-100 focus-within:ring-2 focus-within:ring-offset-2 focus-within:ring-offset-gray-100 focus-within:ring-indigo-500 overflow-hidden">
      <%= image_tag "home3.jpg",  class: "object-cover pointer-events-none group-hover:opacity-75" %>
      <button type="button" className="absolute inset-0 focus:outline-none">
        <span className="sr-only">View details for <%= listing.title %> </span>
      </button>
    </a>
    <p className="mt-2 block text-sm font-medium text-gray-900 truncate pointer-events-none"><%= listing.title %></p>
    <p className="block text-sm font-medium text-gray-500 pointer-events-none"><%= listing.nightly_price %></p>
  </li>
  <% end %>
</ul>

<div class="mt-4">
  <%== pagy_nav(@pagy) %>
</div>